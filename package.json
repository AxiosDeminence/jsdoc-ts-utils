{
  "name": "jsdoc-ts-utils",
  "description": "A plugin with utilities to make your TypeScript-like docs JSDoc valid",
  "homepage": "https://github.com/homer0/jsdoc-ts-utils",
  "version": "0.0.0-development",
  "repository": "homer0/jsdoc-ts-utils",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "license": "MIT",
  "dependencies": {
    "jsdoc": "^3.6.4"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^7.4.0",
    "eslint-plugin-homer0": "^5.0.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "leasot": "^11.1.0",
    "lint-staged": "^10.2.11"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./utils/hooks/pre-commit",
      "post-merge": "./utils/hooks/post-merge",
      "prepare-commit-msg": "./utils/hooks/prepare-commit-msg"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "main": "src/plugin.js",
  "scripts": {
    "lint": "./utils/scripts/lint",
    "lint:full": "./utils/scripts/lint-full",
    "test": "./utils/scripts/test",
    "todo": "./utils/scripts/todo"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
